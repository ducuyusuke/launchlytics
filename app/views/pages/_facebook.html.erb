                  </div>

                  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

                  <script>
                    const ctx11 = document.getElementById('calendarChart11');
                    const data11 = JSON.parse(ctx.dataset.graphData)

                    new Chart(ctx11, {
                      type: 'polarArea',
                      data: {
                        labels: ['Sex: Male', 'Sex: Female', 'Age: 30+', 'City: Brazil', 'CTR: R$0,62'],
                        datasets: [{
                          label: '# of Votes',
                          data: [30, 55, 25, 30, 40],
                          borderWidth: 1
                        }]
                      },
                      options: {
                        scales: {
                          y: {
                            beginAtZero: true
                          }
                        }
                      }
                    });
                  </script>
              </div>
            </div>
          </div>
        </div>
