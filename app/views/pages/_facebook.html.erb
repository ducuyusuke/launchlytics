<div class="page-container">
  <div class="container-overview">
    <div class="overview">
      <div class="overview-charts">
        <div class="overview-first-box">
          <div class="overview-ctr-charts">
            <div class="overview-chart_1">
              <%# First chart %>
                <div id="calendar_chart10">

                  <div style="position: relative;top: 3px;width:500px;height:300px;left: 33px;">

                    <canvas id="calendarChart10" data-graph-data="<%=@graph_data.to_json%>"></canvas>

                  </div>

                  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

                  <script>
                    const ctx10 = document.getElementById('calendarChart10');

                    const gData10 = JSON.parse(ctx.dataset.graphData)[0]
                    // console.log("data", gData.datasets[0].data)
                    // console.log("label", gData.datasets[0].label)
                    // console.log("labels", gData.labels)


                    new Chart(ctx10, {
                      type: 'bar',
                      data: {

                        labels: gData10.labels,
                        datasets: [{
                          label: gData10.datasets[0].label,
                          data: gData10.datasets[0].data,

                          borderWidth: 1
                        }]
                      },
                      options: {
                        scales: {
                          y: {
                            beginAtZero: true
                          }
                        }
                      }
                    });
                  </script>
              </div>
            </div>
          </div>
          <div class="overview-conver-charts">
            <div class="lateral-chart">
              <div class="overview-chart_2">


                  <div style="position: relative; top: 83px; width: 480px; height: 266px;">
