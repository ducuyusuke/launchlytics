                <div id="calendar_chart">

                  <div style="position: relative;top: 3px;width:500px;height:300px;left: 33px;">
                    <canvas id="calendarChart11" data-graph-data="<%=@graph_data%>"></canvas>
                  </div>

                  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

                  <script>
                    const ctx13 = document.getElementById('calendarChart11');
                    const data13 = JSON.parse(ctx.dataset.graphData)

                    new Chart(ctx13, {
                      type: 'bar',
                      data: {
                        labels: ['Red', 'Blue', 'Yellow', 'Green', 'Purple', 'Orange'],
                        datasets: [{
                          label: '# of Votes',
                          data: data,
                          borderWidth: 1
                        }]
                      },
                      options: {
                        scales: {
                          y: {
                            beginAtZero: true
                          }
                        }
                      }
                    });
                  </script>
              </div>
